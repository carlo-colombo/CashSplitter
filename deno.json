{
  "tasks": {
    "dev": "deno run --watch main.ts",
    "build": "deno run --allow-read --allow-write --allow-run build.ts",
    "start": "deno run --allow-net --allow-read --watch dev_server.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/expect": "jsr:@std/expect@^1.0.16",
    "@std/testing": "jsr:@std/testing@^1.0.14",
    "bencode": "npm:bencode@2.0.3",
    "preact": "npm:preact@10.19.2",
    "preact/hooks": "npm:preact@10.19.2/hooks",
    "preact/jsx-runtime": "npm:preact@10.19.2/jsx-runtime",
    "preact-render-to-string": "npm:preact-render-to-string@6.2.1",
    "@testing-library/preact": "npm:@testing-library/preact@3.2.3"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "lib": ["deno.window", "dom", "dom.iterable", "dom.asynciterable"]
  }
}
